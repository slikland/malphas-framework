class PreloaderView extends BaseView
	constructor: () ->
		super null, 'PreloaderView'

	createStart:(evt=null)=>
		super

	create:(evt=null)=>
		super

	createComplete:(evt=null)=>
		super
		
	showStart:(evt=null)=>
		super

	show:(evt=null)=>
		super

	showComplete:(evt=null)=>
		super

	@set progress:(p_value)->
		@text = p_value

	hideStart:(evt=null)=>
		super

	hide:(evt=null)=>
		super

	hideComplete:(evt=null)=>
		super

	destroy:(evt=null)=>
		super

